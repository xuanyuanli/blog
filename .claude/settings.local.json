{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "HTTP_PROXY": "http://127.0.0.1:10808"
  },
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(copy:*)",
      "Bash(cp:*)",
      "Bash(npx:*)",
      "Bash(xcopy:*)",
      "Bash(powershell:*)",
      "Bash(robocopy:*)",
      "Bash(find:*)",
      "Bash(npm:*)",
      "Bash(grep:*)",
      "WebFetch(domain:uniapp.dcloud.net.cn)",
      "Bash(rg:*)",
      "Bash(rm:*)",
      "Bash(move:*)",
      "Bash(mv:*)",
      "Bash(eslint:*)",
      "Bash(ren:*)",
      "Bash(del:*)",
      "Bash(node:*)",
      "Bash(prettier:*)",
      "Bash(git:*)",
      "Bash(cat:*)",
      "Bash(for*)",
      "Bash(dir:*)",
      "Bash(rd:*)",
      "Bash(sed:*)",
      "WebFetch(domain:mp.weixin.qq.com)",
      "Bash(test:*)"
    ],
    "deny": [],
    "defaultMode": "acceptEdits"
  },
  "enableAllProjectMcpServers": true
}